<p id="notice"><%= notice %></p>

<h2>曽祖父世代</h2>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Gender</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @great_grand_parents.each do |great_grand_parent| %>
      <tr>
        <td><%= great_grand_parent.first_name %></td>
        <td><%= great_grand_parent.last_name %></td>
        <td><%= great_grand_parent.gender %></td>
        <td><%= link_to 'Show', great_grand_parent %></td>
        <td><%= link_to 'Edit', edit_great_grand_parent_path(great_grand_parent) %></td>
        <td><%= link_to 'Destroy', great_grand_parent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to '祖父世代へ', grand_parents_path %>
<br>
<br>

<%= link_to 'New Great Grand Parent', new_great_grand_parent_path %>
